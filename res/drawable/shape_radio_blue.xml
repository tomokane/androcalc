<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape android:shape="rectangle">
			<solid android:color="#8297BF" />
			<size android:height="1dp" />
		</shape>
	</item>
	<item android:top="1dp">
		<shape android:shape="rectangle">
			<gradient
				android:angle="270"
				android:startColor="#6984B2"
				android:endColor="#32477E"
				android:type="linear" />
			<padding android:left="48dp" />
		</shape>
	</item>
</layer-list>
